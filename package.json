{
  "name": "@zachwatkins/pdf-data-js",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachwatkins/pdf-data-js.git"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "main": "index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node .",
    "download": "node tasks/download.js",
    "build": "node tasks/build.js",
    "open": "node tasks/open.js",
    "pdf": "node tasks/pdf.js",
    "compile": "gulp compile",
    "watch": "gulp watch"
  },
  "author": {
    "name": "Zachary Kendall Watkins",
    "email": "zwatkins.it@gmail.com",
    "url": "https://zachwatkins.dev/"
  },
  "license": "MIT",
  "dependencies": {
    "@uswds/uswds": "^3.3.0",
    "d3": "^7.8.2",
    "puppeteer": "^19.5.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@uswds/compile": "^1.0.0"
  },
  "config": {
    "url": "public/index.html",
    "download": {
      "url": "https://www.dol.gov/sites/dolgov/files/WB/media/stats/nationaldatabaseofchildcareprices.xlsx",
      "dest": "./data/nationaldatabaseofchildcareprices.xlsx",
      "force": false
    },
    "build": [
      {
        "name": "Texas",
        "file": "./data/texas.xlsx",
        "locked": true,
        "sheets": [
          {
            "name": "Sheet",
            "source": "./data/nationaldatabaseofchildcareprices.xlsx",
            "where": [
              "State_Name",
              "Texas"
            ]
          }
        ]
      },
      {
        "name": "Brazos",
        "file": "./data/brazos.xlsx",
        "sheets": [
          {
            "name": "Sheet",
            "source": "./data/texas.xlsx",
            "select": [
              "State_Name",
              "County_Name",
              "StudyYear",
              "H_Under6_BothWork",
              "H_Under6_FWork",
              "H_Under6_MWork",
              "H_Under6_SingleM",
              "MC18to23",
              "MC24to29",
              "MC36to41"
            ],
            "from": "Sheet",
            "where": [
              "County_Name",
              "Brazos County"
            ],
            "orderBy": [
              "StudyYear",
              "descending"
            ]
          }
        ]
      }
    ],
    "pdf": {
      "url": "./public/index.html",
      "dest": "./build/index.pdf",
      "options": {
        "format": "Letter",
        "printBackground": true,
        "displayHeaderFooter": false
      }
    }
  }
}
