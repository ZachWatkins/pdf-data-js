{
  "name": "@zachwatkins/pdf-data-js",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachwatkins/pdf-data-js.git"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "main": "index.js",
  "config": {
    "url": "public/index.html",
    "download": {
      "url": "https://www.dol.gov/sites/dolgov/files/WB/media/stats/nationaldatabaseofchildcareprices.xlsx",
      "dest": "./data/nationaldatabaseofchildcareprices.xlsx",
      "force": false
    },
    "build": {
      "texas": {
        "src": "./data/nationaldatabaseofchildcareprices.xlsx"
      },
      "brazos": {}
    },
    "data": {
      "remote": "https://www.dol.gov/sites/dolgov/files/WB/media/stats/nationaldatabaseofchildcareprices.xlsx",
      "download": {
        "url": "https://www.dol.gov/sites/dolgov/files/WB/media/stats/nationaldatabaseofchildcareprices.xlsx",
        "dest": "./data/nationaldatabaseofchildcareprices.xlsx"
      },
      "filtered": {
        "src": "./data/nationaldatabaseofchildcareprices.xlsx",
        "dest": "./data/stateprices.xlsx",
        "json": "./data/stateprices.json",
        "sheet": "Sheet",
        "filter": {
          "sheet": "in",
          "only": {
            "State_Name": "Texas"
          }
        }
      }
    },
    "query": {},
    "pdf": {
      "url": "./public/index.html",
      "dest": "./pdf/index.pdf",
      "options": {
        "format": "Letter",
        "printBackground": true,
        "displayHeaderFooter": false
      }
    }
  },
  "scripts": {
    "prestart": "npm run download && npm run build",
    "start": "npm run open",
    "download": "node tasks/download.js",
    "build": "npm run query",
    "open": "node tasks/open.js",
    "query": "node tasks/save-filtered-workbook.js",
    "postquery": "npm run query:tojson",
    "query:tojson": "node tasks/workbook-to-json.js",
    "data": "npm run download && npm run query && npm run query:tojson",
    "pdf:index": "node tasks/pdf-index.js"
  },
  "author": {
    "name": "Zachary Kendall Watkins",
    "email": "zwatkins.it@gmail.com",
    "url": "https://zachwatkins.dev/"
  },
  "license": "MIT",
  "dependencies": {
    "xlsx": "^0.18.5",
    "puppeteer": "^19.5.2"
  }
}
