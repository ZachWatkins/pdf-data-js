{
  "name": "@zachwatkins/pdf-data-js",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/zachwatkins/pdf-data-js.git"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "main": "index.js",
  "config": {
    "url": "public/index.html",
    "download": {
      "url": "https://www.dol.gov/sites/dolgov/files/WB/media/stats/nationaldatabaseofchildcareprices.xlsx",
      "dest": "./data/nationaldatabaseofchildcareprices.xlsx",
      "force": false
    },
    "build": {
      "texas": {
        "src": "./data/nationaldatabaseofchildcareprices.xlsx",
        "sheet": "in",
        "dest": "./data/texas.xlsx",
        "select": [
          "State_Name",
          "County_Name",
          "StudyYear",
          "H_Under6_BothWork",
          "H_Under6_FWork",
          "H_Under6_MWork",
          "H_Under6_SingleM"
        ],
        "where": {
          "State_Name": "Texas"
        }
      },
      "brazos": {
        "src": "./data/texas.xlsx",
        "sheet": "in",
        "dest": "./data/brazos.json",
        "select": [
          "State_Name",
          "County_Name",
          "StudyYear",
          "H_Under6_BothWork",
          "H_Under6_FWork",
          "H_Under6_MWork",
          "H_Under6_SingleM"
        ],
        "where": {
          "State_Name": "Texas",
          "County_Name": "Brazos County"
        }
      }
    },
    "pdf": {
      "url": "./public/index.html",
      "dest": "./pdf/index.pdf",
      "options": {
        "format": "Letter",
        "printBackground": true,
        "displayHeaderFooter": false
      }
    }
  },
  "scripts": {
    "check-env": "node -e 'console.log(process.env)' | grep npm",
    "download": "node tasks/download.js",
    "prestart": "npm run download && npm run build",
    "start": "npm run open",
    "open": "node tasks/open.js",
    "build": "node tasks/build.js",
    "pdf:index": "node tasks/pdf-index.js"
  },
  "author": {
    "name": "Zachary Kendall Watkins",
    "email": "zwatkins.it@gmail.com",
    "url": "https://zachwatkins.dev/"
  },
  "license": "MIT",
  "dependencies": {
    "xlsx": "^0.18.5",
    "puppeteer": "^19.5.2"
  }
}
