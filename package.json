{
  "name": "@zachwatkins/pdf-data-js",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "type": "module",
  "repository": "https://github.com/zachwatkins/pdf-data-js.git",
  "engines": {
    "node": ">=18.13.0"
  },
  "main": "index.js",
  "scripts": {
    "start": "vite",
    "download": "node tasks/download.js",
    "snap": "node tasks/screenshot.js",
    "build": "vite build",
    "build:data": "node tasks/build.js",
    "build:pdf": "node tasks/pdf.js",
    "open": "node tasks/open.js --url=build/index.html",
    "preview": "vite preview",
    "lint": "prettier --check --config .config/.prettierrc.json --ignore-path .config/.prettierignore .",
    "format": "prettier --write --config .config/.prettierrc.json --ignore-path .config/.prettierignore ."
  },
  "author": "Zachary Kendall Watkins <zwatkins.it@gmail.com> (https://zachwatkins.dev/)",
  "license": "MIT",
  "dependencies": {
    "puppeteer": "^20.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.3",
    "prettier": "^3.0.0",
    "vite": "^4.4.7"
  },
  "config": {
    "dest": "build",
    "download": {
      "url": "https://www.dol.gov/sites/dolgov/files/WB/media/stats/nationaldatabaseofchildcareprices.xlsx",
      "dest": "data/nationaldatabaseofchildcareprices.xlsx",
      "force": false
    },
    "pdf": {
      "options": {
        "path": "pdf/index.pdf",
        "format": "Letter",
        "printBackground": true,
        "displayHeaderFooter": false
      }
    },
    "build": {
      "workbooks": [
        {
          "name": "Texas",
          "file": "./data/texas.xlsx",
          "locked": true,
          "sheets": [
            {
              "name": "Sheet",
              "source": "./data/nationaldatabaseofchildcareprices.xlsx",
              "where": [
                "State_Name",
                "Texas"
              ]
            }
          ]
        },
        {
          "name": "Brazos",
          "file": "./data/brazos.xlsx",
          "sheets": [
            {
              "name": "Sheet",
              "source": "./data/texas.xlsx",
              "select": [
                "State_Name",
                "County_Name",
                "StudyYear",
                "H_Under6_BothWork",
                "H_Under6_FWork",
                "H_Under6_MWork",
                "H_Under6_SingleM",
                "MC18to23",
                "MC24to29",
                "MC36to41"
              ],
              "from": "Sheet",
              "where": [
                "County_Name",
                "Brazos County"
              ],
              "orderBy": [
                "StudyYear",
                "descending"
              ]
            }
          ]
        }
      ]
    }
  }
}
